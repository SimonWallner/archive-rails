<% puts object %>
<% puts label %>
<% if !object.nil? && object.any? %>
	<div class="fact">
	  <span class="label">
	    <%= label.to_s %>:
	  </span>
	  <ul>
	    <% object.each do |f| %>
		    <li>
	            <%= just_links_markdown f.content %>
		    </li>
	    <% end %>
	  </ul>
	</div>
<% end %>